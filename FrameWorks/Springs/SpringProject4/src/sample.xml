<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"http://www.springframework.org/dtd/spring-beans-2.0.dtd">
			

<beans>
   
   <bean id="s3" class="org.jsp.app.Student">
   <property name="studentId" value="10"></property>
   <property name="studentName" value="xyz"></property>
   <property name="address" ref="a1"></property>
   </bean>
   <bean id="a1" class="org.jsp.app.Address">
   <property name="houseNum" value="170"></property>
   <property name="area" value="kphb"></property>
   <property name="location" value="hyd"></property>
   </bean>

</beans>