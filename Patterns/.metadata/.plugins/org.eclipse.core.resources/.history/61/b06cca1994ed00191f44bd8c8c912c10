package org.jsp.app;

import java.util.Scanner;

public class Pattern1 
{
 public static void main(String[] args) 
 {
	 int num;
	 Scanner scanner=new Scanner(System.in);
	 int s1=scanner.nextInt(num);
	 for(int i=1;i<=s1;i++)
  {
	  for(int j=1;j<=s1;j++)
	  {
		 System.out.print("*");
	  }
	  System.out.println();
  }
}
}
