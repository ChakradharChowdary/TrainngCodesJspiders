package org.app.programs;

import java.util.Scanner;

public class SumOfFristFivePrimeNumbers 
{
 public static void main(String[] args) {
	Scanner scanner=new Scanner(System.in);
	 int noOfPrimeNos=30;
	 int sum=0;
	 for(int i=2;i<=noOfPrimeNos;i++)
	 {
		 int number=0;
		 for(int j=2;j<i;j++)
		 {
			 if(i%j==0)
			 {
				 number=1;
			 }
			 if(number==0)
			 {
				 System.out.println(i);
				 sum=sum+i;
			 }
		 }
		 System.out.println("The sum Of the 10 prime Numbers are="+sum);
	 }
}
}
